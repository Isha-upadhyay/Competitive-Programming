#include <bits/stdc++.h>
using namespace std;
 
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int x , mx = INT_MIN;
        for(int i=0;i<n;i++){
            cin >> x;
            mx = max(mx, x);
        }
        cout << mx << "\n";
    }
    return 0;
}
