#include <bits/stdc++.h>
using namespace std;
const int MOD = 1e9 + 7;

// Write your code here
int main(){
ios::sync_with_stdio(false);
cin.tie(nullptr);

int T;
cin >> T;
while(T--){
    int N, k;
    cin >> N >> k;

    vector<vector<long long>> dp(N + 1, vector<long long>(k+1, 0));
    dp[0][0] = 1;

    for(int i=1;i<=N;i++){
        long long sum = 0;
        for(int j=0;j<=k;j++){
            sum = (sum + dp[i-1][j]) % MOD;
        }
        dp[i][0] = (sum * 21) % MOD;

        for(int j=1;j<=k;j++){
            dp[i][j] = (dp[i-1][j-1]*5) % MOD;
        }
    }

    long long ans = 0;
    for(int j=0;j<=k;j++) ans = (ans + dp[N][j]) % MOD;
    cout << ans << "\n";
}
return 0;
}

