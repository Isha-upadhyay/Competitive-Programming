// Write your code here

#include <bits/stdc++.h>
using namespace std;

vector<int> precomputeUglyNumbers(int maxN){
    vector<int> ugly(maxN);
    ugly[0] = 1;

    int i2 = 0, i3=0, i5=0;
    int next2 = 2, next3 = 3, next5 = 5;

    for(int i=1;i<maxN;i++){
        int nextUgly = min(next2, min(next3, next5));
        ugly[i] = nextUgly;

        if(nextUgly == next2){
            i2++;
            next2 = ugly[i2] * 2;
        }
         if(nextUgly == next3){
            i3++;
            next3 = ugly[i3] * 3;
        }
         if(nextUgly == next5){
            i5++;
            next5 = ugly[i5] * 5;
        }
    }
    return ugly;
}

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;

    vector<int> queries(t);

    int maxN = 0;
    for(int i=0;i<t;i++){
        cin >> queries[i];
        maxN = max(maxN, queries[i]);
    }

    vector<int> uglyNumbers = precomputeUglyNumbers(maxN);

    for(int i=0;i<t;i++){
        cout << uglyNumbers[queries[i]-1] << "\n";
    }

    return 0;
}
