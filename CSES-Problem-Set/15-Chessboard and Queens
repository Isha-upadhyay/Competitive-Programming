#include <iostream>
#include <vector>
using namespace std;

const int N = 8;
char board[N][N];
int res = 0;

void solve(int row, vector<bool>& col, vector<bool>& diag1, vector<bool>& diag2) {
    if (row == N) {
        res++;
        return;
    }
    for (int c = 0; c < N; ++c) {
        if (board[row][c] == '*' || col[c] || diag1[row + c] || diag2[row - c + N - 1])
            continue;
        col[c] = diag1[row + c] = diag2[row - c + N - 1] = true;
        solve(row + 1, col, diag1, diag2);
        col[c] = diag1[row + c] = diag2[row - c + N - 1] = false;
    }
}

int main() {
    for (int i = 0; i < N; ++i)
        for (int j = 0; j < N; ++j)
            cin >> board[i][j];

    vector<bool> col(N, false), diag1(2 * N, false), diag2(2 * N, false);
    solve(0, col, diag1, diag2);
    cout << res << endl;
    return 0;
}
