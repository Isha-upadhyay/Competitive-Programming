#include <iostream>
using namespace std;


int main(){
    int n;
    cin >> n;

    for(long long k = 1; k<=n; ++k){
        long long total_ways = (k * k) * (k * k - 1) / 2;
        long long attacking_ways = 0;

        if(k>2){
            attacking_ways = 4*(k-1) * (k-2);
        }

        cout << (total_ways - attacking_ways) << endl;
    }

    return 0;
}
